<?xml version="1.0"?>
<rdf:RDF xmlns="http://logic-guard-layer.ai/ontology/maintenance#"
     xml:base="http://logic-guard-layer.ai/ontology/maintenance"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:maintenance="http://logic-guard-layer.ai/ontology/maintenance#">

    <owl:Ontology rdf:about="http://logic-guard-layer.ai/ontology/maintenance">
        <rdfs:label>Maintenance Domain Ontology</rdfs:label>
        <rdfs:comment>Ontology for technical maintenance domain validation in Logic-Guard-Layer with physics-based constraints</rdfs:comment>
        <owl:versionInfo>2.0.0</owl:versionInfo>
    </owl:Ontology>

    <!-- ==================== CLASSES ==================== -->

    <!-- Base Component Class -->
    <owl:Class rdf:about="#Komponente">
        <rdfs:label>Komponente</rdfs:label>
        <rdfs:comment>Base class for all technical components</rdfs:comment>
    </owl:Class>

    <!-- Violation Marker Classes (inferred by reasoner) -->
    <owl:Class rdf:about="#ConstraintViolation">
        <rdfs:label>Constraint Violation</rdfs:label>
        <rdfs:comment>Marker class for components that violate constraints</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#RangeViolation">
        <rdfs:subClassOf rdf:resource="#ConstraintViolation"/>
        <rdfs:label>Range Violation</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#PhysicsViolation">
        <rdfs:subClassOf rdf:resource="#ConstraintViolation"/>
        <rdfs:label>Physics Violation</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#CavitationRisk">
        <rdfs:subClassOf rdf:resource="#PhysicsViolation"/>
        <rdfs:label>Cavitation Risk</rdfs:label>
        <rdfs:comment>NPSH available less than NPSH required</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#CarnotViolation">
        <rdfs:subClassOf rdf:resource="#PhysicsViolation"/>
        <rdfs:label>Carnot Limit Violation</rdfs:label>
        <rdfs:comment>COP exceeds Carnot limit</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#EfficiencyViolation">
        <rdfs:subClassOf rdf:resource="#PhysicsViolation"/>
        <rdfs:label>Efficiency Violation</rdfs:label>
        <rdfs:comment>Efficiency exceeds 100%</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#LifespanExceeded">
        <rdfs:subClassOf rdf:resource="#RangeViolation"/>
        <rdfs:label>Lifespan Exceeded</rdfs:label>
        <rdfs:comment>Operating hours exceed maximum lifespan</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#NegativeValue">
        <rdfs:subClassOf rdf:resource="#RangeViolation"/>
        <rdfs:label>Negative Value</rdfs:label>
        <rdfs:comment>Value that must be non-negative is negative</rdfs:comment>
    </owl:Class>

    <!-- Rotating Components -->
    <owl:Class rdf:about="#RotierendeKomponente">
        <rdfs:subClassOf rdf:resource="#Komponente"/>
        <rdfs:label>Rotierende Komponente</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Motor">
        <rdfs:subClassOf rdf:resource="#RotierendeKomponente"/>
        <rdfs:label>Motor</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Elektromotor">
        <rdfs:subClassOf rdf:resource="#Motor"/>
        <rdfs:label>Elektromotor</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Pumpe">
        <rdfs:subClassOf rdf:resource="#RotierendeKomponente"/>
        <rdfs:label>Pumpe</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Kreiselpumpe">
        <rdfs:subClassOf rdf:resource="#Pumpe"/>
        <rdfs:label>Kreiselpumpe</rdfs:label>
        <rdfs:comment>Centrifugal pump - subject to cavitation</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Hydraulikpumpe">
        <rdfs:subClassOf rdf:resource="#Pumpe"/>
        <rdfs:label>Hydraulikpumpe</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Vakuumpumpe">
        <rdfs:subClassOf rdf:resource="#Pumpe"/>
        <rdfs:label>Vakuumpumpe</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Kompressor">
        <rdfs:subClassOf rdf:resource="#RotierendeKomponente"/>
        <rdfs:label>Kompressor</rdfs:label>
        <rdfs:comment>Compressor - isentropic compression thermodynamics</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Generator">
        <rdfs:subClassOf rdf:resource="#RotierendeKomponente"/>
        <rdfs:label>Generator</rdfs:label>
    </owl:Class>

    <!-- Heat Transfer Equipment -->
    <owl:Class rdf:about="#Waermetauscher">
        <rdfs:subClassOf rdf:resource="#Komponente"/>
        <rdfs:label>Wärmetauscher</rdfs:label>
        <rdfs:comment>Heat exchanger - energy balance Q_hot = Q_cold</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Waermepumpe">
        <rdfs:subClassOf rdf:resource="#Komponente"/>
        <rdfs:label>Wärmepumpe</rdfs:label>
        <rdfs:comment>Heat pump - COP limited by Carnot</rdfs:comment>
    </owl:Class>

    <!-- Energy Storage -->
    <owl:Class rdf:about="#Energiespeicher">
        <rdfs:subClassOf rdf:resource="#Komponente"/>
        <rdfs:label>Energiespeicher</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Batteriespeicher">
        <rdfs:subClassOf rdf:resource="#Energiespeicher"/>
        <rdfs:label>Batteriespeicher</rdfs:label>
        <rdfs:comment>Battery storage - SOC 0-100%, cycle limits</rdfs:comment>
    </owl:Class>

    <!-- Static Components -->
    <owl:Class rdf:about="#StatischeKomponente">
        <rdfs:subClassOf rdf:resource="#Komponente"/>
        <rdfs:label>Statische Komponente</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Ventil">
        <rdfs:subClassOf rdf:resource="#StatischeKomponente"/>
        <rdfs:label>Ventil</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Regelventil">
        <rdfs:subClassOf rdf:resource="#Ventil"/>
        <rdfs:label>Regelventil</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Absperrventil">
        <rdfs:subClassOf rdf:resource="#Ventil"/>
        <rdfs:label>Absperrventil</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Behaelter">
        <rdfs:subClassOf rdf:resource="#StatischeKomponente"/>
        <rdfs:label>Behälter</rdfs:label>
    </owl:Class>

    <!-- Sensors -->
    <owl:Class rdf:about="#Sensor">
        <rdfs:subClassOf rdf:resource="#Komponente"/>
        <rdfs:label>Sensor</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Drucksensor">
        <rdfs:subClassOf rdf:resource="#Sensor"/>
        <rdfs:label>Drucksensor</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Temperatursensor">
        <rdfs:subClassOf rdf:resource="#Sensor"/>
        <rdfs:label>Temperatursensor</rdfs:label>
    </owl:Class>

    <!-- Power Generation -->
    <owl:Class rdf:about="#Stromerzeugung">
        <rdfs:subClassOf rdf:resource="#Komponente"/>
        <rdfs:label>Stromerzeugung</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Windturbine">
        <rdfs:subClassOf rdf:resource="#Stromerzeugung"/>
        <rdfs:label>Windturbine</rdfs:label>
        <rdfs:comment>Wind turbine - Betz limit 59.3%</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Gasturbine">
        <rdfs:subClassOf rdf:resource="#Stromerzeugung"/>
        <rdfs:label>Gasturbine</rdfs:label>
        <rdfs:comment>Gas turbine - Brayton cycle efficiency</rdfs:comment>
    </owl:Class>

    <!-- Events -->
    <owl:Class rdf:about="#Ereignis">
        <rdfs:label>Ereignis</rdfs:label>
        <rdfs:comment>Base class for all events</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Wartungsereignis">
        <rdfs:subClassOf rdf:resource="#Ereignis"/>
        <rdfs:label>Wartungsereignis</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Ausfallereignis">
        <rdfs:subClassOf rdf:resource="#Ereignis"/>
        <rdfs:label>Ausfallereignis</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Messereignis">
        <rdfs:subClassOf rdf:resource="#Ereignis"/>
        <rdfs:label>Messereignis</rdfs:label>
    </owl:Class>

    <!-- Disjoint Classes -->
    <owl:AllDisjointClasses>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="#Wartungsereignis"/>
            <rdf:Description rdf:about="#Ausfallereignis"/>
            <rdf:Description rdf:about="#Messereignis"/>
        </owl:members>
    </owl:AllDisjointClasses>

    <owl:AllDisjointClasses>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="#Pumpe"/>
            <rdf:Description rdf:about="#Motor"/>
            <rdf:Description rdf:about="#Sensor"/>
            <rdf:Description rdf:about="#Ventil"/>
        </owl:members>
    </owl:AllDisjointClasses>

    <!-- Other -->
    <owl:Class rdf:about="#Anlage">
        <rdfs:label>Anlage</rdfs:label>
        <rdfs:comment>A plant or system containing components</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Techniker">
        <rdfs:label>Techniker</rdfs:label>
        <rdfs:comment>A maintenance technician</rdfs:comment>
    </owl:Class>

    <!-- ==================== OBJECT PROPERTIES ==================== -->

    <owl:ObjectProperty rdf:about="#hatKomponente">
        <rdfs:domain rdf:resource="#Anlage"/>
        <rdfs:range rdf:resource="#Komponente"/>
        <rdfs:label>hat Komponente</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#istTeilVon">
        <rdfs:domain rdf:resource="#Komponente"/>
        <rdfs:range rdf:resource="#Anlage"/>
        <rdfs:label>ist Teil von</rdfs:label>
        <owl:inverseOf rdf:resource="#hatKomponente"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hatWartung">
        <rdfs:domain rdf:resource="#Komponente"/>
        <rdfs:range rdf:resource="#Wartungsereignis"/>
        <rdfs:label>hat Wartung</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#durchgefuehrtVon">
        <rdfs:domain rdf:resource="#Wartungsereignis"/>
        <rdfs:range rdf:resource="#Techniker"/>
        <rdfs:label>durchgeführt von</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hatViolation">
        <rdfs:domain rdf:resource="#Komponente"/>
        <rdfs:range rdf:resource="#ConstraintViolation"/>
        <rdfs:label>hat Violation</rdfs:label>
        <rdfs:comment>Links component to its constraint violations</rdfs:comment>
    </owl:ObjectProperty>

    <!-- ==================== DATATYPE PROPERTIES ==================== -->

    <!-- Operating Hours with range constraint >= 0 -->
    <owl:DatatypeProperty rdf:about="#hatBetriebsstunden">
        <rdfs:domain rdf:resource="#Komponente"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:label>hat Betriebsstunden</rdfs:label>
        <rdfs:comment>Operating hours of the component (must be >= 0)</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Max Lifespan with range constraint > 0 -->
    <owl:DatatypeProperty rdf:about="#hatMaxLebensdauer">
        <rdfs:domain rdf:resource="#Komponente"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:minExclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:label>hat maximale Lebensdauer</rdfs:label>
        <rdfs:comment>Maximum lifespan in hours (must be > 0)</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Maintenance Interval with range constraint > 0 -->
    <owl:DatatypeProperty rdf:about="#hatWartungsintervall">
        <rdfs:domain rdf:resource="#Komponente"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:minExclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:label>hat Wartungsintervall</rdfs:label>
        <rdfs:comment>Maintenance interval in hours (must be > 0 and &lt;= max lifespan)</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hatSeriennummer">
        <rdfs:domain rdf:resource="#Komponente"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>hat Seriennummer</rdfs:label>
    </owl:DatatypeProperty>

    <!-- Pressure with range 0-350 bar for standard hydraulics -->
    <owl:DatatypeProperty rdf:about="#hatDruckBar">
        <rdfs:domain rdf:resource="#Komponente"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">350</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:label>hat Druck (bar)</rdfs:label>
        <rdfs:comment>Pressure in bar (standard hydraulics: 0-350 bar)</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Temperature with typical range -40 to 150 C -->
    <owl:DatatypeProperty rdf:about="#hatTemperaturC">
        <rdfs:domain rdf:resource="#Komponente"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">-40</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">150</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:label>hat Temperatur (°C)</rdfs:label>
        <rdfs:comment>Temperature in Celsius (typical range: -40 to +150)</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hatDatum">
        <rdfs:domain rdf:resource="#Ereignis"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:label>hat Datum</rdfs:label>
    </owl:DatatypeProperty>

    <!-- RPM with range 0-10000 -->
    <owl:DatatypeProperty rdf:about="#hatDrehzahl">
        <rdfs:domain rdf:resource="#RotierendeKomponente"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10000</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:label>hat Drehzahl (rpm)</rdfs:label>
        <rdfs:comment>Rotation speed in RPM (typically 0-10000)</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hatStatus">
        <rdfs:domain rdf:resource="#Komponente"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>hat Status</rdfs:label>
        <rdfs:comment>Status of the component (aktiv, defekt, wartung)</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Efficiency 0-100% (First Law of Thermodynamics) -->
    <owl:DatatypeProperty rdf:about="#hatWirkungsgrad">
        <rdfs:domain rdf:resource="#Komponente"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">100</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:label>hat Wirkungsgrad (%)</rdfs:label>
        <rdfs:comment>Efficiency in percent (0-100%, First Law of Thermodynamics)</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- NPSH Available -->
    <owl:DatatypeProperty rdf:about="#hatNPSHverfuegbar">
        <rdfs:domain rdf:resource="#Pumpe"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:label>hat NPSH verfügbar (m)</rdfs:label>
        <rdfs:comment>Net Positive Suction Head available in meters</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- NPSH Required -->
    <owl:DatatypeProperty rdf:about="#hatNPSHerforderlich">
        <rdfs:domain rdf:resource="#Pumpe"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:label>hat NPSH erforderlich (m)</rdfs:label>
        <rdfs:comment>Net Positive Suction Head required in meters</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- COP for heat pumps -->
    <owl:DatatypeProperty rdf:about="#hatCOP">
        <rdfs:domain rdf:resource="#Waermepumpe"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0</xsd:minExclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:label>hat COP</rdfs:label>
        <rdfs:comment>Coefficient of Performance (must be > 0, limited by Carnot)</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Source/Sink temperatures for heat pump -->
    <owl:DatatypeProperty rdf:about="#hatQuelltemperatur">
        <rdfs:domain rdf:resource="#Waermepumpe"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>hat Quelltemperatur (°C)</rdfs:label>
        <rdfs:comment>Source temperature (ambient/ground) in Celsius</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hatVorlauftemperatur">
        <rdfs:domain rdf:resource="#Waermepumpe"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>hat Vorlauftemperatur (°C)</rdfs:label>
        <rdfs:comment>Sink/flow temperature (heating water) in Celsius</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Battery SOC 0-100% -->
    <owl:DatatypeProperty rdf:about="#hatLadezustand">
        <rdfs:domain rdf:resource="#Batteriespeicher"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">100</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:label>hat Ladezustand (%)</rdfs:label>
        <rdfs:comment>State of Charge in percent (0-100%)</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Battery cycles -->
    <owl:DatatypeProperty rdf:about="#hatLadezyklen">
        <rdfs:domain rdf:resource="#Batteriespeicher"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:label>hat Ladezyklen</rdfs:label>
        <rdfs:comment>Number of charge cycles</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hatMaxLadezyklen">
        <rdfs:domain rdf:resource="#Batteriespeicher"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:minExclusive>
